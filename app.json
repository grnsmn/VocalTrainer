{
  "expo": {
    "name": "Vocal Trainer",
    "slug": "VocalTrainer",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c6e9ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.VocalTrainer.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/splash.png"
    },
    "extra": {
      "eas": {
        "projectId": "25baeacb-0df5-4607-b105-0e6242835ec3"
      }
    },
    "owner": "gscripts",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ]
  }
}
